/******************************************************************************

                            Even or Odd 

*******************************************************************************/

public class Main
{
	public static void main(String[] args) {
		
		int num = 123452;
		if(num!=0)
		{
		if(num%2==0)
		    System.out.println("Even number - " + num);
		else
		    System.out.println("Odd number - " + num);
		}
		else
		    System.out.println("Zero number - " + num);
	}
}
